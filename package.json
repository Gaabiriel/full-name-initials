{
  "name": "metronic-badge-name-initials",
  "version": "2.0.3",
  "description": "Get fullname initials",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/badgenameinitials.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/badgenameinitials.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gaabiriel/metronic-badge-name-initials.git"
  },
  "keywords": [
    "name",
    "initials"
  ],
  "author": "Gabriel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gaabiriel/metronic-badge-name-initials/issues"
  },
  "homepage": "https://github.com/Gaabiriel/metronic-badge-name-initials#readme",
  "devDependencies": {
    "@angular/compiler": "^5.1.3",
    "@angular/compiler-cli": "^5.1.3",
    "rollup": "^0.53.4",
    "typescript": "^2.6.2",
    "uglify-js": "^3.3.5"
  },
  "dependencies": {
    "@angular/common": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3"
  }
}
